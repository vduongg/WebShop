@model List<KaiserStore.Models.ImportDetails>;

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["page"] = "inventory";
    ViewData["Title"] = "Lịch sử nhập hàng hóa";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutSidebar.cshtml";
}

<!DOCTYPE html>

<html>
<head>

    <link rel="stylesheet" href="~/css/StyleAdminCategory.css" />
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
</head>
<body>
    <div class="category">
        <div class="category-header">
            <h3>
               Lịch sử nhập hàng
            </h3>
     

        </div>

        <div class="category-body">
            <table class="table">
                <thead>
                    <tr>
                        <th>STT</th>
                        <th>MÃ SẢN PHẨM</th>
                        <th>SIZE</th>
                        <th>SỐ LƯỢNG</th>
                        <th>GIÁ NHẬP</th>
                        <th>THỜI GIAN NHẬP</th>
                    </tr>
                </thead>
                <tbody>

                    @for (int i = 0; i < Model.Count(); i++)
                    {
                        <tr>

                            <td>@(i + 1)</td>
                            <td>@Model[i].ProductId </td>
                            <td>@Model[i].Size</td>
                            <td>@Model[i].quantity</td>
                            <td>@Model[i].price</td>
                            <td>@Model[i].DateTime</td>

                           
                        </tr>

                    }


                </tbody>
            </table>
        </div>

    </div>
    <script src="~/js/AdminCategory.js"></script>
</body>
</html>



